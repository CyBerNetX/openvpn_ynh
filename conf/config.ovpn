client
dev tun
proto udp
resolv-retry infinite
nobind
persist-key
persist-tun
comp-lzo yes

remote DOMAINTOCHANGE 1194
route-delay
reneg-sec 0
redirect-gateway
script-security 2
auth-user-pass
ca ./ca.crt
auth-nocache
cipher AES-256-CBC
auth SHA512
tls-version-min 1.2
tls-client
tls-cipher TLS-DHE-RSA-WITH-AES-256-GCM-SHA384

auth-nocache
user openvpn
group openvpn
